<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/textViewEmpty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tidak_ada_hasil"
        android:textAlignment="center"
        android:textColor="?colorOnPrimary"
        android:visibility="gone"
        app:drawableTint="?colorOnPrimary"
        app:drawableTopCompat="@drawable/baseline_error_outline" />

    <TextView
        android:id="@+id/textViewError"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/terjadi_kesalahan_dalam_mengambil_data"
        android:textAlignment="center"
        android:textColor="?colorOnPrimary"
        android:visibility="gone"
        app:drawableTint="?colorOnPrimary"
        app:drawableTopCompat="@drawable/baseline_warning_amber" />

</FrameLayout>


